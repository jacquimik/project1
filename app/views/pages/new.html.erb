<form action="item" method="post">
	<label>Name:</label>
	<input type="text" name="item" value="<%= @item %>" autofocus>
	<label>Image:</label>
	<input type="text" name="image_url" value="<%= @item %>">
	<label>Description:</label>
	<input type="text" name="description" value="<%= @item %>">


	<button>Upload Item!</button>
</form>